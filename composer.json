{
    "name": "assistant-engine/open-functions-slack",
    "description": "The Slack Open Function package integrates Slack with large language models (LLMs) using the Open Functions architecture, enabling streamlined repository management and file operations.",
    "homepage": "https://github.com/AssistantEngine/open-functions-slack",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Weigang",
            "email": "florian@assistant-engine.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AssistantEngine\\OpenFunctions\\Slack\\": "src/"
        }
    },

    "require": {
        "assistant-engine/open-functions-core": "^1.0",
        "guzzlehttp/guzzle": "^7.9"
    }
}
